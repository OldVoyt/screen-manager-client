<Window x:Class="ScreenManagerClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScreenManagerClient"
        xmlns:wpf="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
        mc:Ignorable="d"
        Title="Screen Manager"
        Topmost="False"
        ShowInTaskbar="True"
        ResizeMode="NoResize"
        WindowState="Normal">
    <Grid Background="Black">
        <Grid ZIndex="1" Margin="0" Name="MainGrid" Visibility="Hidden">
        <!--
        <wpf:ChromiumWebBrowser Name="WebBrowser"  Address="https://mooon-schedule.vercel.app/"></wpf:ChromiumWebBrowser>
    -->
        </Grid>
        <Grid ZIndex="10" Background="Black" Opacity="1" Name="FadingGrid">
            
        </Grid>
        <Grid ZIndex="1">
        <MediaElement Margin="0" Name="MediaElement"></MediaElement>
        </Grid>
    </Grid>
</Window>
